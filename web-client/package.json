{
	"name": "open-food-today",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "eslint .",
		"wrangler:deploy": "npx wrangler deploy .svelte-kit/cloudflare/_worker.js",
		"deploy": "bun run build && bun run wrangler:deploy"
	},
	"devDependencies": {
		"@celar-ui/svelte": "^0.0.15",
		"@eslint/compat": "^1.2.9",
